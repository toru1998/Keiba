var ifrBody = ifrObj.contentWindow.document.body;
var ifrHeight = ifrBody.scrollHeight;
var movePosition = boxPositionTop + (boxHeight - ifrHeight) / 2;
$('#colorbox').css('top', movePosition +'px');
